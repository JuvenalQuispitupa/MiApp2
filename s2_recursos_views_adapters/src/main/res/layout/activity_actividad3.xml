<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.jmarkstar.sesion2.Actividad3">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tv_Actividad3_TextoRecibido"
        android:text="Texto Recibido del anterior Activity"
        android:layout_centerInParent="true"/>
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/et_Actividad3_recibido"
        android:layout_below="@+id/tv_Actividad3_TextoRecibido"
        android:textColor="@color/colorAccent"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Retroceder/Atras"
        android:onClick="onBack"
        android:layout_below="@+id/et_Actividad3_recibido"
        android:layout_centerHorizontal="true"/>

</RelativeLayout>
